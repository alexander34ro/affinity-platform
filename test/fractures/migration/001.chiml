ins: action, config
out: output
verbose: 0
do:

  # chimera test/fractures/migration/001.chiml up "{\"mongoUrl\":\"localhost/test\"}"

  - ins:
      - mongoUrl: config.mongoUrl
        collectionName: 'test'
    out: dbConfig

  - if: action == 'up'
    do:
      - ins:
          - dbConfig
          - 'insert'
          - {'name': 'Odin'}
        do: |[$.loadJs('chimera-framework', 'mongo.execute')]
        out: output

    else:
      - ins:
          - dbConfig
          - 'remove'
          - {'name':'Odin'}
        do: |[$.loadJs('chimera-framework', 'mongo.execute')]
        out: output
