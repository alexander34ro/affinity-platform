ins: action, config
out: output
verbose: 4
do:

  # chimera test/fractures/migration/001.chiml up "{\"mongoUrl\":\"localhost/test\"}"

  - |$.loadJs('chimera-framework', 'mongo') --> mongo
  - |mongo.db(config.mongoUrl) --> db
  - |mongo.collection(db, 'test') --> test

  - if: action == 'up'
    do:
      - |({'name':'Odin'}) -> [test.insert] -> output
    else:
      - |({'name':'Odin'}) -> [test.remove] -> output

  - |()=>{db.close();}