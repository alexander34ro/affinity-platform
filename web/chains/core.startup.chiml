ins: state
out: state
vars:

  # routes
  routes:
    - route: '/'
      method: 'all'
      chain: 'index.chiml'

do:

  # preprocess routes
  - |(routes, _chain_cwd) -> {state.config.preprocessRoutes} -> state.config.routes

  # upgrade 
  - |(state.config.migrationConfig) -> [$.loadJs('chimera-framework', 'migration.upgrade')]
