ins: st
out: st
vars:

  # routes
  routes:
    - route: '/'
      method: 'all'
      chain: 'index.chiml'

do:

  # preprocess routes
  - |(routes, _chain_cwd) -> {$.getPreprocessedRoutes} -> st.config.routes

  # do upgrade migration 
  - |($.migrationConfig) -> [$.loadJs('chimera-framework', 'migration.upgrade')]
