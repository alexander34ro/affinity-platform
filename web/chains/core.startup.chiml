ins: state
out: state
do:

  # preprocess routes
  - |(state.config.routes, _chain_cwd) -> {$.getPreprocessedRoutes} -> state.config.routes

  # do migration
  - if: state.config.lastateVersion
    do: |($.migrationConfig) -> [$.loadJs('chimera-framework', 'migration.upgrade')]
    else: |($.migrationConfig, state.config.version) -> [$.loadJs('chimera-framework', 'migration.upgrade')]
