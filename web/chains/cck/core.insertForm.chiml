ins: state
out: response
do:
  - |(state) -> [$.cck.getInitialState] -> cckState
  - |(state.config.errorTemplate) --> response.view

  # get presentation
  - |response.data.cckState <-- cckState
  - |(cckState) -> [$.cck.getInputRow] -> response.data.input

  # set single show
  - |(cckState.schema.insertFormView) --> response.view
