ins: fieldName, value, row, cckState 
out: html
do:
  - |webConfig <-- cckState.state.config
  - |(webConfig.viewPath + 'cck/inputs/option.ejs') -> [$.loadJs('fs', 'readFile')] --> ejsContent
  - |(ejsContent, {fieldName, value, row, cckState.schema.fields[fieldName].options}) -> {$.loadJs('ejs', 'render'} -> html