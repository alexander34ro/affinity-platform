ins: state
out: response
do:
  - |(state) -> [$.cck.getInitialState] -> cckState
  - |(cckState.schema.updateActionView) --> response.view

  # get data from API
  - parallel:
      - do:
          - |(_chain_cwd + 'core.update.chiml', state, cckState) -> [$.runChain] -> apiResponse
          - |response.data <-- apiResponse.data
      - |response.data.cckState <-- cckState
      - |response.partial <-- cckState.schema.partial