ins: state
out: response
do:
  - |(state) -> [$.cck.getInitialState] -> cckState

  # do select
  - if: |!$.util.isNullOrUndefined(cckState.schema.selectChain)
    do: |(cckState.schema.selectChain, cckState) -> [$.runChain] -> cckState

  # do post-processing
  - if: |cckState.result.status == 200 && !$.util.isNullOrUndefined(cckState.schema.afterSelectChain)
    do: |(cckState.schema.afterSelectChain, cckState) -> [$.runChain] -> cckState

  - |response.data <-- cckState.result