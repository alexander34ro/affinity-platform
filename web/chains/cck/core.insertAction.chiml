ins: state
out: response
do:
  - |(state) -> [$.cck.getInitialState] -> cckState
  - |(cckState.schema.insertActionView) --> response.view

  # get data from API
  - |(_chain_cwd + 'core.insert.chiml', state, cckState) -> [$.runChain] -> apiResponse
  - |response.data <-- apiResponse.data
  - |response.data.cckState <-- cckState
  - |response.partial <-- cckState.partial
