ins: state
out: state
do:

  - |dbConfig <- [$.helper.getDbConfig]
  - |(state.config, dbConfig) -> {$.util.getPatchedObject} -> state.config

  # get `routes` `cckRoutes`, `dbRoutes`.
  # combine them into new `state.config.routes`
  - |(state.config) -> [$.helper.getDbRoutes] -> dbRoutes
  - |routes <-- state.config.routes
  - |cckRoutes <- {$.cck.getRoutes}
  - |(routes, cckRoutes) -> {$.merge} -> state.config.routes
  - |(state.config.routes, dbRoutes) -> {$.merge} -> state.config.routes
