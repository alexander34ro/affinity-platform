ins: cckState
out: cckState
do:
  - |collectionName <-- cckState.schema.collectionName
  - |documentId <-- cckState.documentId
  - |filter <-- {} 
  - if: |!$.util.isNullOrUndefined(documentId)
    do: |filter <-- {'_id': documentId}
  - |(collectionName, 'find', filter) -> [$.helper.mongoExecute] -> cckState.result