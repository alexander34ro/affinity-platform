ins: state
out: response
do:
  - |jwtTokenName <-- state.config.jwtTokenName

  - |auth <- {$.helper.getLoggedOutAuth} 
  - ins:
      - auth 
      - config.jwtSecret
    out: jwtToken
    do: |{$.loadJs('jsonwebtoken', 'sign')}

  - |({token: jwtToken, status: 200, userMessage: 'Logout success', developerMessage: 'Logout success'}) --> response.data
  - |(jwtToken) --> response.cookies[jwtTokenName]
