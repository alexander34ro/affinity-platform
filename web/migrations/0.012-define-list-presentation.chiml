ins: action, migrationConfig
out: output
do:

  - if: "action == 'up'"
    do: 
      - ins:
          - 'web_configs'
          - 'update'
          - key: 'cck'
          - $set:
              value.presentation.list: <%- viewPath %>cck/presentations/list.ejs
        out: output
        do: "[$.helper.mongoExecute]"
