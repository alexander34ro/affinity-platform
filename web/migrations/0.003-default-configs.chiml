ins: action, migrationConfig
out: output
do:
    - if: action == 'up'
      do:
        - |{$.cck.getDefaultConfigs} -> defaultConfigs
        - |('web_configs', 'insert', defaultConfigs) -> [$.helper.mongoExecute] -> output

      else:
        - |('web_configs', 'remove', {'defaultConfig': 1}) -> [$.helper.mongoExecute] -> output