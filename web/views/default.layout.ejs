<!DOCTYPE html>
<html lang="en">
  <head>
    <%- partial.htmlHeader %>
  </head>
  <body>
    <div class="container">
      <div class="row">
        <%- partial.largeBanner %>
      </div>
    </div>
    <div class="container">
      <div class="row">

        <% if (partial.leftWidget.trim().length > 0) { %>
          <div class="col-sm-3">
            <%- partial.leftWidget %>
          </div>
        <% } %>

        <% if (partial.leftWidget.trim().length > 0 && partial.rightWidget.trim().length > 0) { %>
         <div class="col-sm-6">
        <% } else if (partial.leftWidget.trim().length > 0 || partial.rightWidget.trim().length > 0) { %>
         <div class="col-sm-9">
        <% } else {%>
         <div class="col-sm-12">
        <% } %>
          <%- partial.smallBanner %>
          <%- content %>
          <%- partial.smallFooter %>
        </div>

        <% if (partial.rightWidget.trim().length > 0) { %>
          <div class="col-sm-3">
            <%- partial.rightWidget %>
          </div>
        <% } %>

      </div>
    </div>
    <div class="container">
      <div class="row">
        <%- partial.largeFooter %>
      </div>
    </div>
    <%- partial.htmlFooter %>
  </body>
</html>
