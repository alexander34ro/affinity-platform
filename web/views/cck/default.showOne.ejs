<% for (let fieldName of cckState.fieldNames) { %>
  <div class="form-group row">
    <label class="col-form-label col-sm-2"><%= cckState.schema.fields[fieldName].caption %></label>
    <div class="col-sm-10">
      <%- render(cckState.schema, fieldName, 'presentationTemplate', result) %>
    </div>
  </div>
<% } %>
<div class="form-group row">
  <div class="col-sm-10 col-sm-offset-2">
    <a class="btn btn-default" href="/data/<%= cckState.schemaName %>/update/<%= cckState.documentId %>">Edit</a>
    <a class="btn btn-default" href="/data/<%= cckState.schemaName %>">Back</a>
  </div>
</div>
