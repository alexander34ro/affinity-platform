<% if (status < 400) { %>
  <div class="alert alert-success" role="alert">
    <strong>Success</strong> <%= userMessage %>
  </div>
<% } else { %>
  <div class="alert alert-danger" role="alert">
    <strong>Failed</strong> <%= userMessage %>
  </div>
<% } %>
<div style="margin-bottom:1em">
  <a class="btn btn-default" href="/data/<%= cckState.schemaName %>">Back</a>
</div>
<script>
  console.error('<%= developerMessage %>');
</script>
